   "source": [
    "#1.Creating an Empty Series\n",
    "import pandas as pd\n",
    "ser=pd.Series()\n",
    "print(ser)"
   ]
  },
  {
   "cell_type": "raw",
   "id": "443c10a3",
   "metadata": {},
   "source": [
    "#2. Non Empty Series\n",
    "#a. Python sequence\n",
    "a=[1,4,6,8]\n",
    "b=['a','b','c','d']\n",
    "ser=pd.Series(a,b)\n",
    "print(ser)"
   ]
  },
  {
   "cell_type": "raw",
   "id": "90aa9d9d",
   "metadata": {},
   "source": [
    "a=range(5)\n",
    "ser=pd.Series(a)\n",
    "print(ser)"
   ]
  },
  {
   "cell_type": "raw",
   "id": "1c81ab1a",
   "metadata": {},
   "source": [
    "#3.NDARRAY\n",
    "#a.linspace\n",
    "import numpy as np\n",
    "ser=pd.Series(np.linspace(24,64,5))\n",
    "print(ser)"
   ]
  },
  {
   "cell_type": "raw",
   "id": "50ce36fe",
   "metadata": {},
   "source": [
    "#b. arange\n",
    "a=np.arange(5)\n",
    "ser=pd.Series(a)\n",
    "print(ser)"
   ]
  },
  {
   "cell_type": "raw",
   "id": "643cd414",
   "metadata": {},
   "source": [
    "#c.tile\n",
    "a=np.tile(['a','b'],5)\n",
    "ser=pd.Series(a)\n",
    "print(ser)"
   ]
  },
  {
   "cell_type": "raw",
   "id": "b1f4a0aa",
   "metadata": {},
   "source": [
    "#4.A Dictionary\n",
    "a={'a':1,'b':2,'c':3}\n",
    "ser=pd.Series(a)\n",
    "print(ser)"
   ]
  },
  {
   "cell_type": "raw",
